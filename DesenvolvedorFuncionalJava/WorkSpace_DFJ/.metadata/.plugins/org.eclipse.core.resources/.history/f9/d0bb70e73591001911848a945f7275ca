package exer9;

import org.junit.Test;
import exer8.Forno;

public class Exer9 {

	@Test
	public void exer9() {
		Forno forno = new Forno();
		forno.assar(() -> System.out.println("Pizza, lambida"));
		forno.assar(()-> {
			System.out.println("molho");
			System.out.println("queijo");
			System.out.println("azeitona lambda");
		});
		
				
				

	}

}
